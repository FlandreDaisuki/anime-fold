{
  "env": {
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:node/recommended"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 11
  },
  "rules": {
    "node/no-unsupported-features/node-builtins": ["error", { "version": ">=12.0.0" }],
    "node/no-unsupported-features/es-builtins": ["error", { "version": ">=12.0.0" }]
  }
}
